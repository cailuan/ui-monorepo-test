{
    "compilerOptions": {
      "module": "ESNext",
      "target": "ES5",
      "jsx": "react",
      "declaration": true,
      "declarationDir": "./dist/types",
      "outDir": "./dist",
      "esModuleInterop": true,
      "strict": true,
      "skipLibCheck": true,
      "sourceMap": true,
      "moduleResolution": "node",
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules", "dist"]
  }